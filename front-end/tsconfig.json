{
    "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "es5",
      "moduleResolution": "node",
      "outDir": "dist",
      "rootDir": ".",
      "lib": [ "es2016", "dom" ],
      "allowJs": true,
      "jsx": "react",
      "strict": true,
      "noImplicitAny": true,
      "noImplicitThis": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "inlineSourceMap": true,
      "downlevelIteration": true
    }
  }
  